    "format-json": "prettier -w ./public/*"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "peerDependencies": {
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^6.0.0",
    "@types/node": "^18.17.1",
    "eslint-config-custom": "workspace:*",
    "rollup": "^3.28.1",
    "rollup-plugin-ts": "^3.4.4",
    "tsconfig": "workspace:*",
    "types": "workspace:*",
    "tsx": "^3.12.7",
    "yaml": "^2.3.2"
  }
}
